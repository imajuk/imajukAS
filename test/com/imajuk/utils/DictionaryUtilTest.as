package com.imajuk.utils{	import flash.utils.Dictionary;		import org.libspark.as3unit.after;	import org.libspark.as3unit.assert.*;	import org.libspark.as3unit.before;	import org.libspark.as3unit.test;	use namespace test;	use namespace before;	use namespace after;	internal class DictionaryUtilTest	{		before function setupSample () : void		{		}				test function count():void		{			var dictionary:Dictionary = new Dictionary();			dictionary["a"] = "a";			dictionary["b"] = "b";			dictionary["c"] = "c";			dictionary["d"] = "d";			//個数を数える			assertEquals(4, DictionaryUtil.count(dictionary));			//nullは個数に含めない			dictionary["e"] = null;			assertEquals(4, DictionaryUtil.count(dictionary));			//nullを個数に含める			assertEquals(5, DictionaryUtil.count(dictionary, false));		}		test function containes():void		{			var dictionary:Dictionary = new Dictionary();			dictionary["a"] = "a";			dictionary["b"] = "b";			dictionary["c"] = "c";			dictionary["d"] = "d";			assertEquals(true, DictionaryUtil.containes(dictionary , "a"));			assertEquals(false, DictionaryUtil.containes(dictionary , "z"));		}				test function normalize():void		{			var dictionary:Dictionary = new Dictionary();            dictionary["a"] = "a";            dictionary["b"] = null;            dictionary["c"] = "c";                        var normalized:Dictionary = DictionaryUtil.normalize(dictionary);            assertEquals(2, DictionaryUtil.count(normalized, false));        }    }}