package com.imajuk.utils{    import flash.display.*;    import org.libspark.as3unit.after;    import org.libspark.as3unit.assert.*;    import org.libspark.as3unit.before;    import org.libspark.as3unit.test;        use namespace test;        use namespace before;        use namespace after;    internal class URIUtilTest extends Sprite    {        before function setupSample():void        {        }        test function getFileName():void        {            assertEquals("index.html", URIUtil.getFileName("http://www.imgsrc.co.jp/index.html"));            assertEquals("index.html", URIUtil.getFileName("http://www.imgsrc.co.jp/dir1/index.html"));            assertEquals("index.html", URIUtil.getFileName("http://www.imgsrc.co.jp/dir1/dir2/index.html"));            assertEquals("index.html", URIUtil.getFileName("http://www.imgsrc.co.jp/index.html?id=111"));            assertEquals("index.html", URIUtil.getFileName("/index.html"));            assertEquals("hoge.jpg", URIUtil.getFileName("hoge.jpg"));            assertEquals(null, URIUtil.getFileName("http://www.imgsrc.co.jp/"));            assertEquals(null, URIUtil.getFileName("http://www.imgsrc.co.jp/"));            assertEquals(null, URIUtil.getFileName("http://www.imgsrc.co.jp"));            assertEquals(null, URIUtil.getFileName("http://imgsrc.co.jp"));            assertEquals(null, URIUtil.getFileName("http://imgsrc.jp"));        }
        test function getFileExtention():void        {            assertEquals("html", URIUtil.getFileExtention("http://www.imgsrc.co.jp/index.html"));            assertEquals("cgi", URIUtil.getFileExtention("http://www.imgsrc.co.jp/index.cgi"));            assertEquals(null, URIUtil.getFileExtention("http://www.imgsrc.co.jp/"));            assertEquals("cgi", URIUtil.getFileExtention("http://www.imgsrc.co.jp/index.cgi?id=123456789"));            assertEquals(null, URIUtil.getFileExtention("http://www.imgsrc.co.jp"));            assertEquals(null, URIUtil.getFileExtention("http://imgsrc.jp"));        }

        after function teardownSample():void        {        }    }}