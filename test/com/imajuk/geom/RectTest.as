package com.imajuk.geom{    import flash.display.*;
    import flash.geom.Point;	    import org.libspark.as3unit.after;    import org.libspark.as3unit.assert.*;    import org.libspark.as3unit.before;    import org.libspark.as3unit.test;    use namespace test;    use namespace before;    use namespace after;
    internal class RectTest extends Sprite    {        before function setupSample():void        {        }
                                test function create():void        {            var r:Rect = new Rect(1, 2, 3, 4);            assertEquals(1, r.x);            assertEquals(2, r.y);            assertEquals(3, r.width);            assertEquals(4, r.height);			            //移動とサイズ変更            r.x = 5;            r.y = 6;            r.width = 7;            r.height = 8;            assertEquals(5, r.x);            assertEquals(6, r.y);            assertEquals(7, r.width);            assertEquals(8, r.height);        }                                test function clone():void        {            var c:Rect = new Rect(1, 2, 3, 4);            var c2:Rect = c.clone() as Rect;            assertFalse(c2 == null);            assertTrue(c2 is Rect);            assertFalse(c == c2);			            assertTrue(c.equals(c2));			        }                                test function equals():void        {            var c:Rect = new Rect(1, 2, 3, 4);			            //異なる座標            assertFalse(c.equals(new Rect(1.1, 2, 3, 4)));			            //異なるサイズ            assertFalse(c.equals(new Rect(1, 2, 3, 4.1)));						            //異なるIGeomとの比較            assertFalse(c.equals(new Circle(1, 2, 0.2)));            assertFalse(c.equals(new Doughnut(1, 2, 0.2, 0.1)));        }                                test function size():void
        {            var r:Rect = new Rect(1, 2, 3, 4);            assertEquals(new Point(3, 4), r.size as Point);        }
                                after function teardownSample():void        {        }    }}