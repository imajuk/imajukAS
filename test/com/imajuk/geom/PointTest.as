package com.imajuk.geom{	import flash.display.Sprite;	import org.libspark.as3unit.after;	import org.libspark.as3unit.assert.*;	import org.libspark.as3unit.before;	import org.libspark.as3unit.test;	use namespace test;	use namespace before;	use namespace after;	internal class PointTest extends Sprite	{		before function setupSample () : void		{		}								test function create () : void		{			var p : Point2D = new Point2D(1, 2);			assertTrue(p is IGeom);			assertEquals(1, p.x);			assertEquals(2, p.y);						//移動			p.x = 5;			p.y = 6;			assertEquals(5, p.x);			assertEquals(6, p.y);		}								test function compare () : void		{			var p1 : Point2D = new Point2D(1, 2);			var p2 : Point2D = p1.clone() as Point2D;			assertFalse(p2 == null);			assertTrue(p1.equals(p2));			assertFalse(p1 == p2);						p2.x = 3;			assertFalse(p1.equals(p2));		}								test function containsPoint () : void		{			var p1 : Point2D = new Point2D(1, 2);			var p2 : Point2D = new Point2D(1, 2);			var p3 : Point2D = p1.clone() as Point2D;						assertTrue(p1.containsPoint(p2));			assertTrue(p1.containsPoint(p3));						p2.x = 3;			p3.x = 3;			assertFalse(p1.containsPoint(p2));			assertFalse(p1.containsPoint(p3));		}								after function teardownSample () : void		{		}	}}