package com.imajuk.render2D.bmpTrim{    import com.imajuk.constructions.DocumentClass;    import com.imajuk.logs.Logger;    import com.imajuk.utils.PerformanceChecker;    import org.libspark.thread.EnterFrameThreadExecutor;    import org.libspark.thread.Thread;    import flash.display.Sprite;    import flash.display.StageQuality;    /**     * @author shinyamaharu     */    public class Render2D_bmptrim_Sample extends DocumentClass     {        public function Render2D_bmptrim_Sample()        {            super(StageQuality.HIGH);                        Thread.initialize(new EnterFrameThreadExecutor());                        Logger.release("Render2DSampleMain");        }        override protected function start() : void        {        	var layer:Sprite = Sprite(addChild(new Sprite()));        	            new Render2D_bmptrim_SampleThread(layer).start();                        PerformanceChecker.instance.initialize(this);        };    }}