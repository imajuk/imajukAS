package com.imajuk.events {
	import flash.events.Event;	/**     * 非同期処理に関するイベントを定義します.     *      * <p>this defines about asyncronus functions.</p>     * @author yamaharu     */    public class AsyncEvent extends Event     {        //--------------------------------------------------------------------------        //        //  Class constants        //        //--------------------------------------------------------------------------        /**         * 非同期処理が開始された事を表す定数です.         *          * <p>The constant defines to start asynchronus function.</p>          */        public static const START:String = "asyncStart";                /**         * 非同期処理が終了した事を表す定数です.         *          * <p>The constant defines to stop asynchronus function.</p>          */        public static const STOP:String = "asyncStop";                /**         * 非同期処理が実行されている事を表す定数です.         *          * <p>The constant defines to update asynchronus function.</p>          */        public static const UPDATE:String = "asyncUpdate";        //--------------------------------------------------------------------------        //        //  Constructor        //        //--------------------------------------------------------------------------                public function AsyncEvent(type:String, 		                           bubbles:Boolean = false,		                           cancelable:Boolean = false)        {            super(type, bubbles, cancelable);        }    }
}
